<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.youtube_analysis.Controllers.MainDialogController">
   <children>
      <Label fx:id="nameField" contentDisplay="CENTER" layoutX="10.0" layoutY="14.0" prefHeight="63.0" prefWidth="580.0" text="Label" textAlignment="CENTER" AnchorPane.bottomAnchor="323.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="14.0">
         <font>
            <Font name="System Bold" size="32.0" />
         </font></Label>
       <Label fx:id="aboutField" layoutX="10.0" layoutY="82.0" prefHeight="233.0" prefWidth="580.0" text="Label" AnchorPane.bottomAnchor="82.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
      <ButtonBar layoutX="14.0" layoutY="311.0" prefHeight="50.0" prefWidth="580.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
        <buttons>
            <Button mnemonicParsing="false" onAction="#showTaskDialog" text="Перейти в YouTube Analytics">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/icon.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
          <Button mnemonicParsing="false" onAction="#showSettingDialog" prefHeight="38.0" prefWidth="290.0" text="Перейти в Настройки">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/settings.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
